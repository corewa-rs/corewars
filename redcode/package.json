{
    "name": "redcode",
    "displayName": "redcode",
    "description": "Language per REDCODE Reference Standard: ICWS'94 draft (extended)",
    "version": "0.0.3",
    "license": "MIT",
    "publisher": "ian-h-chamberlain",
    "repository": {
        "type": "git",
        "url": "https://github.com/ian-h-chamberlain/corewa_rs.git",
        "directory": "redcode"
    },
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "core",
        "war",
        "core war",
        "wars",
        "core wars",
        "redcode"
    ],
    "contributes": {
        "languages": [
            {
                "id": "redcode",
                "aliases": [
                    "Redcode",
                    "redcode"
                ],
                "extensions": [
                    ".red"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "redcode",
                "scopeName": "source.redcode",
                "path": "./syntaxes/redcode.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "build": "npx js-yaml syntaxes/redcode.tmLanguage.yaml > syntaxes/redcode.tmLanguage.json",
        "package": "vsce package",
        "deploy": "vsce publish"
    },
    "devDependencies": {
        "js-yaml": "^3.14.0",
        "vsce": "^1.76.1"
    }
}
